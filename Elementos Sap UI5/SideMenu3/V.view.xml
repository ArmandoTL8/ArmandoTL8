<mvc:View
	controllerName="sap.tnt.sample.SideNavigation.C"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	height="100%"
	class="side-navigation-demo">
	<Button
		text="Cerrar"
		icon="sap-icon://menu2"
		press=".onCollapseExpandPress" />
	<tnt:SideNavigation id="sideNavigation">
		<tnt:NavigationList>
			<tnt:NavigationListItem text="Idioma / Languaje" icon="sap-icon://translate">
				<tnt:NavigationListItem text="Inglés" />
				<tnt:NavigationListItem text="Español" />
				<tnt:NavigationListItem text="Portugués" id="subItem3" key="subItem3" />
			</tnt:NavigationListItem>
			<tnt:NavigationListItem text="Avisos" icon="sap-icon://message-information">
				<tnt:NavigationListItem text="Aviso CFDI 3.3 (Español)" />
				<tnt:NavigationListItem text="Código de Conducta" />
				<tnt:NavigationListItem text="Política de EHS y Calidad" />
			</tnt:NavigationListItem>
			<tnt:NavigationListItem text="Documentos" icon="sap-icon://legend">
				<tnt:NavigationListItem text="Manuales proveedor (Español)" />
				<tnt:NavigationListItem text="Manuales pagos proveedores (Español)" />
			</tnt:NavigationListItem>
			<tnt:NavigationListItem text="Videos" icon="sap-icon://play">
				<tnt:NavigationListItem text="Videos proveedor (Español)" />
			</tnt:NavigationListItem>
		</tnt:NavigationList>
		<tnt:fixedItem>
			<tnt:NavigationList>
				<tnt:NavigationListItem text="Cambiar contraseña" icon="sap-icon://unlocked" />
				<tnt:NavigationListItem text="Cerrar sesión" icon="sap-icon://private" />
			</tnt:NavigationList>
		</tnt:fixedItem>
	</tnt:SideNavigation>
</mvc:View>